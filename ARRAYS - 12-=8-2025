1) mISSING NUMBER

class Solution {
    public int missingNumber(int[] nums) {
        int n = nums.length;
        int value = n * (n+1)/2;
        int sum = 0;
        int result = 0;
        for(int i = 0; i < n ; i++){
            sum = sum + nums[i];
        }
        result = value - sum;
        return result;
    }
}
